{
  "name": "dapp-dockuments-regisry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ethereum:build": "(cd ./bin/ethereum-network && docker-compose build --force-rm --no-cache)",
    "ethereum:start": "(cd ./bin/ethereum-network && docker-compose up -d)",
    "ethereum:console": "(cd ./bin/ethereum-network && docker-compose exec ethereum-web3-bootnode-console /bin/sh ./console)",
    "ethereum:stop": "(cd ./bin/ethereum-network && docker-compose down)",
    "database:start": "(cd ./bin/postrges-database && docker-compose up -d)",
    "database:stop": "(cd ./bin/postrges-database && docker-compose down)",
    "backend:dev:start:api": "(cd ./src/backend && yarn start:api)",
    "frontend:dev:start": "(cd ./src/frontend && yarn start)"
  }
}
